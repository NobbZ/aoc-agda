data Direction : Set where
  Up   : Direction
  Down : Direction

trackFloor′ : ℤ → List Direction → ℤ
trackFloor′ count [] = count
trackFloor′ count (Up ∷ ds) = trackFloor′ (Int.suc count) ds
trackFloor′ count (Down ∷ ds) = trackFloor′ (Int.pred count) ds

trackFloor : List Direction → ℤ
trackFloor = trackFloor′ 0ℤ

stringToDirections′ : List Char → Maybe (List Direction)
stringToDirections′ [] = Maybe.just []
stringToDirections′ ('(' ∷ cs) with stringToDirections′ cs
...   | Maybe.just result = Maybe.just (Up ∷ result)
...   | Maybe.nothing     = Maybe.nothing
stringToDirections′ (')' ∷ cs) with stringToDirections′ cs
...   | Maybe.just result = Maybe.just (Down ∷ result)
...   | Maybe.nothing     = Maybe.nothing
stringToDirections′ _ = Maybe.nothing

stringToDirections : String → Maybe (List Direction)
stringToDirections s = stringToDirections′ (String.toList s)

upUpDownDownIsZero : trackFloor (Up ∷ Up ∷ Down ∷ Down ∷ []) ≡ 0ℤ
upUpDownDownIsZero = refl

openOpenCloseCloseIsUpUpDownDown : stringToDirections "(())" ≡ Maybe.just (Up ∷ Up ∷ Down ∷ Down ∷ [])
openOpenCloseCloseIsUpUpDownDown = refl

_ : stringToDirections "1" ≡ Maybe.nothing
_ = refl

